{
  "name": "@oneblink/sdk",
  "description": "OneBlink SDK to serve as an entry point for all OneBlink Services in NodeJS",
  "version": "13.1.1-beta.1",
  "author": "OneBlink <developers@oneblink.io> (https://oneblink.io)",
  "bugs": {
    "url": "https://github.com/oneblink/sdk-node-js/issues"
  },
  "dependencies": {
    "@aws-sdk/client-sesv2": "^3.987.0",
    "@oneblink/sdk-core": "^9.1.0-beta.3",
    "@oneblink/storage": "^6.0.0-beta.1",
    "joi": "^18.0.2",
    "jsonwebtoken": "^9.0.3",
    "jwks-rsa": "^3.2.2",
    "nodemailer": "^8.0.1",
    "uuid": "^13.0.0"
  },
  "devDependencies": {
    "@aws-sdk/client-s3": "^3.987.0",
    "@eslint/eslintrc": "^3.3.3",
    "@eslint/js": "^9.39.2",
    "@microsoft/eslint-plugin-sdl": "^1.1.0",
    "@oneblink/release-cli": "^4.0.0",
    "@oneblink/types": "github:oneblink/types",
    "@types/joi": "^17.2.3",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/nodemailer": "^7.0.9",
    "@typescript-eslint/eslint-plugin": "^8.55.0",
    "@typescript-eslint/parser": "^8.55.0",
    "eslint": "^9.39.2",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.5.5",
    "fixpack": "^4.0.0",
    "prettier": "^3.8.1",
    "prettier-plugin-jsdoc": "^1.8.0",
    "typedoc": "^0.28.16",
    "typescript": "^5.9.3",
    "vitest": "^4.0.18"
  },
  "directories": {
    "doc": "docs",
    "example": "examples",
    "test": "test"
  },
  "engines": {
    "node": ">=24",
    "npm": ">=11"
  },
  "files": [
    "tenants"
  ],
  "homepage": "https://oneblink.github.io/sdk-node-js/",
  "keywords": [],
  "license": "MIT",
  "main": "./tenants/oneblink.js",
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/oneblink/sdk-node-js.git"
  },
  "scripts": {
    "build": "tsc --build",
    "docs": "typedoc",
    "eslint": "eslint --fix --cache .",
    "fixpack": "fixpack",
    "prepare": "npm run build",
    "pretest": "npm run eslint && tsc --noEmit && npm run fixpack",
    "release": "oneblink-release repository --no-name",
    "test": "vitest run",
    "types": "npm i -D github:oneblink/types",
    "typescript": "tsc --noEmit",
    "update-dependents": "oneblink-release update-dependents --force"
  },
  "type": "module"
}
