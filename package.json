{
  "name": "@oneblink/sdk",
  "description": "OneBlink SDK to serve as an entry point for all OneBlink Services in NodeJS",
  "version": "0.0.1",
  "bugs": {
    "url": "https://github.com/blinkmobile/oneblink-sdk-js/issues"
  },
  "dependencies": {
    "aws-sdk": "2.272.1",
    "axios": "0.18.0",
    "jsonwebtoken": "8.3.0"
  },
  "devDependencies": {
    "eslint": "^5.1.0",
    "eslint-config-standard": "^11.0.0",
    "eslint-plugin-import": "^2.13.0",
    "eslint-plugin-node": "^6.0.1",
    "eslint-plugin-promise": "^3.8.0",
    "eslint-plugin-standard": "^3.1.0",
    "fixpack": "^2.3.1",
    "flow-bin": "^0.76.0",
    "jest": "^23.4.0",
    "remark-cli": "^5.0.0",
    "remark-lint": "^6.0.2",
    "remark-preset-lint-recommended": "^3.0.2"
  },
  "directories": {
    "doc": "docs",
    "example": "examples",
    "test": "test"
  },
  "engines": {
    "node": ">=8",
    "npm": ">=6"
  },
  "files": [
    "lib",
    "index.js",
    "types.js"
  ],
  "homepage": "https://github.com/blinkmobile/oneblink-sdk-js",
  "keywords": [],
  "license": "MIT",
  "main": "index.js",
  "nyc": {
    "exclude": [
      "dist",
      "examples",
      "test"
    ]
  },
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/blinkmobile/oneblink-sdk-js.git"
  },
  "scripts": {
    "eslint": "eslint --fix --cache .",
    "fixpack": "fixpack",
    "flow_check": "flow check",
    "jest": "jest",
    "lint-md": "remark . --use remark-preset-lint-recommended",
    "pretest": "npm run eslint && npm run flow_check && npm run fixpack && npm run lint-md",
    "test": "npm run jest"
  }
}
